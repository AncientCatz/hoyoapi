{
  "name": "hoyoapi",
  "version": "1.0.0-alpha",
  "description": "HoYoAPI is an unofficial API Wrapper library developed to facilitate communication with the official HoYoLab API.",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier . -w",
    "lint": "eslint . --fix",
    "build": "tsx scripts/build.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavyflow/hoyoapi.git"
  },
  "keywords": [
    "hoyolab-api",
    "hoyoverse-api",
    "genshin-api",
    "honkai-api"
  ],
  "author": "Ashary Vermaysha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wavyflow/hoyoapi/issues"
  },
  "homepage": "https://github.com/wavyflow/hoyoapi#readme",
  "lint-staged": {
    "**/*.(ts,js)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=8.17"
  },
  "packageManager": "^npm@9.0.0",
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@types/eslint": "latest",
    "@types/node": "20.2.5",
    "@types/prettier": "latest",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "esbuild": "^0.17.14",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "tiny-glob": "^0.2.9",
    "tsx": "^3.12.6",
    "typescript": "5.1.3"
  }
}
